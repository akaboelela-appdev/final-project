<h1>
  All coveragegroups
</h1>

<a href="/coveragegroups/new">
  Add a new coveragegroup
</a>

<hr>

<table class="table">
  <tr>
    <th>
      ID
    </th>

    <th>
      Name
    </th>

    <th>
      Created at
    </th>

    <th>
      Updated at
    </th>

    <th>
    </th>
  </tr>

  <% @coveragegroups.each do |coveragegroup| %>
  <tr>
    <td>
      <%= coveragegroup.id %>
    </td>

    <td>
      <%= coveragegroup.name %>
    </td>

    <td>
      <%= time_ago_in_words(coveragegroup.created_at) %> ago
    </td>

    <td>
      <%= time_ago_in_words(coveragegroup.updated_at) %> ago
    </td>

    <td>
      <a href="/coveragegroups/<%= coveragegroup.id %>">
        Show details
      </a>
    </td>
  </tr>
  <% end %>
</table>
